{
  "name": "OneTeam",
  "description": "Practice Cybersecurity Policies in Automated Way!",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "tabs",
    "storage"
  ],

  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "icons": {
    "16": "/images/get_started16.png",
    "32": "/images/get_started32.png",
    "48": "/images/get_started48.png",
    "128": "/images/get_started128.png"
  }
}
